{
  "multipart": [
    {
      "apply": {
        "model": "nepio:block/cable/nep_cable_core"
      }
    },
    {
      "when": { "north":  "true" },
      "apply": {
        "model": "nepio:block/cable/nep_cable_side"
      }
    },
    {
      "when": { "east":  "true" },
      "apply": {
        "model": "nepio:block/cable/nep_cable_side",
        "y": 90
      }
    },
    {
      "when": { "south":  "true" },
      "apply": {
        "model": "nepio:block/cable/nep_cable_side",
        "y": 180
      }
    },
    {
      "when": { "west":  "true" },
      "apply": {
        "model": "nepio:block/cable/nep_cable_side",
        "y": 270
      }
    },
    {
      "when": { "up":  "true" },
      "apply": {
        "model": "nepio:block/cable/nep_cable_side",
        "x": 270
      }
    },
    {
      "when": { "down":  "true" },
      "apply": {
        "model": "nepio:block/cable/nep_cable_side",
        "x": 90
      }
    }
  ]
}